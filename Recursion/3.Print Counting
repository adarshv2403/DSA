#include<iostream>
using namespace std;

void print(int n){

   //Base condition 
   
   if(n==0){
        return;
    }
//dispalying output 

    cout<<n<<" ";

    //recursive call
    print(n-1);
}

int main() {

//taking input

    int n;
    cin>>n;

 //calling function 
 print (n);
}

/*

@TIME COMPLEXITY

the time complexity is O(n) because it calls the function to n times.

@SPACE COMPLEXITY

the space complexity is O(n) becuse it use stack of n size to store it .
